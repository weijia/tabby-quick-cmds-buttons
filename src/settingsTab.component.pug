h3 QuickCmdBtn

.form-line
    .header
        .title Disable tabs
        .description Disable tabs for buttons

    toggle.ml-1(
        title="Disable tabs for buttons",
        [(ngModel)]='config.store.quickCmdBtnPlugin.disableTabs',
        (ngModelChange)='config.save()'
    )

.form-line
    .header
        .title Use system theme
        .description Use system theme

    toggle.ml-1(
        title="Use system theme",
        [(ngModel)]='config.store.quickCmdBtnPlugin.useSystemTheme',
        (ngModelChange)='config.save()'
    )
